{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "attach",
			"name": "Attach",
			"port": 9229,
			"skipFiles": ["<node_internals>/**"]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Execute Command",
			"skipFiles": ["<node_internals>/**"],
			"runtimeExecutable": "node",
			"runtimeArgs": ["--loader", "ts-node/esm", "--no-warnings=ExperimentalWarning"],
			"program": "${workspaceFolder}/bin/dev.js",
			"args": ["hello", "world"]
		},
		{
			"type": "bun",
			"internalConsoleOptions": "neverOpen",
			"request": "launch",
			"name": "Debug Tests",
			"program": "${workspaceFolder}/runtime/gcp/Cloud Storage/check-storage-bucket-public-access.ts",
			"cwd": "${workspaceFolder}",
			"stopOnEntry": false,
			"watchMode": false
		}
	]
}
