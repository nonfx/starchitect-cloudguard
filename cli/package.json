{
  "name": "starchitect-cloudguard",
  "description": "Starchitect Cloudguard is a CLI to run tests against your Cloud accounts or your IAC files.",
  "version": "0.0.0",
  "author": "The Non-Functional Company",
  "bin": {
    "starchitect-cloudguard": "./bin/run.js"
  },
  "bugs": "https://github.com/nonfx/starchitect-cloudguard/issues",
  "dependencies": {
    "@oclif/core": "^4.2.0",
    "@oclif/plugin-help": "^6.2.20",
    "@oclif/plugin-plugins": "^5.4.23",
    "@types/cli-progress": "^3.11.6",
    "cli-progress": "^3.12.0",
    "esbuild": "^0.24.2",
    "inquirer": "^12.3.0",
    "p-queue": "^8.0.1",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0"
  },
  "devDependencies": {
    "@oclif/test": "^4.1.4",
    "oclif": "^4.17.4",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=23.5.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/nonfx/starchitect-cloudguard",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "oclif": {
    "bin": "starchitect-cloudguard",
    "dirname": "starchitect-cloudguard",
    "commands": "./dist/cli/src/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " "
  },
  "repository": "nonfx/starchitect-cloudguard",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b --incremental",
    "dev": "npm run build && ./bin/dev.js",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepack": "oclif manifest && oclif readme",
    "test": "bun test",
    "version": "oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts"
}
