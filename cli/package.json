{
  "name": "starchitect-cloudguard",
  "description": "Starchitect Cloudguard is a CLI to run tests against your Cloud accounts or your IAC files.",
  "version": "0.0.0",
  "author": "The Non-Functional Company",
  "bin": {
    "starchitect-cloudguard": "./bin/run.js"
  },
  "bugs": "https://github.com/nonfx/starchitect-cloudguard/issues",
  "dependencies": {
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5",
    "@types/cli-progress": "^3.11.6",
    "cli-progress": "^3.12.0",
    "inquirer": "^12.3.0",
    "p-queue": "^8.0.1",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0"
  },
  "devDependencies": {
    "@oclif/test": "^4",
    "oclif": "^4",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/nonfx/starchitect-cloudguard",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "oclif": {
    "bin": "starchitect-cloudguard",
    "dirname": "starchitect-cloudguard",
    "commands": "./src/commands",
    "topicSeparator": " ",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ]
  },
  "repository": "nonfx/starchitect-cloudguard",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "dev": "./bin/dev.js",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "bun --bun oclif manifest && bun --bun oclif readme",
    "test": "bun test",
    "version": "bun --bun oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts"
}
