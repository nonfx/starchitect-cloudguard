{
  "name": "starchitect-cloudguard",
  "description": "Starchitect Cloudguard is a CLI to run tests against your Cloud accounts or your IAC files.",
  "version": "0.0.0",
  "author": "The Non-Functional Company",
  "bin": {
    "mynewcli": "./bin/run.js"
  },
  "bugs": "https://github.com/nonfx/starchitect-cloudguard/issues",
  "dependencies": {
    "@aws-sdk/client-sts": "^3.716.0",
    "@aws-sdk/credential-providers": "^3.716.0",
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5",
    "p-queue": "^8.0.1",
    "pino-pretty": "^13.0.0"
  },
  "devDependencies": {
    "@oclif/test": "^4",
    "@types/chai": "^4",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "chai": "^4",
    "eslint": "^8",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "mocha": "^10",
    "oclif": "^4",
    "shx": "^0.3.3",
    "ts-node": "^10",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/nonfx/starchitect-cloudguard",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "oclif": {
    "bin": "starchitect-cloudguard",
    "dirname": "starchitect-cloudguard",
    "commands": "./src/commands",
    "topicSeparator": " ",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ]
  },
  "repository": "nonfx/starchitect-cloudguard",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "dev": "./bin/dev.js",
    "lint": "eslint . --ext .ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts"
}
